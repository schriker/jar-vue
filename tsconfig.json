{
    "compilerOptions": {
        "outDir": "./built/",
        "sourceMap": true,
        "strict": false,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "experimentalDecorators": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}